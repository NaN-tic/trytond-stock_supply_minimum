<data>
    <xpath expr="/tree" position="replace_attributes">
        <tree colors="If(Bool(Eval('minimum_quantity')), 'red', 'black')"/>
    </xpath>
    <xpath expr="/tree/field[@name='quantity']" position="before">
        <field name="minimum_quantity"/>
    </xpath>
</data>
